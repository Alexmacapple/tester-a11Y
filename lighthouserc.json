{
  "ci": {
    "collect": {
      "url": [
        "https://alexmacapple.github.io/tester-a11Y/",
        "https://alexmacapple.github.io/tester-a11Y/les-contrastes",
        "https://alexmacapple.github.io/tester-a11Y/la-langue",
        "https://alexmacapple.github.io/tester-a11Y/les-images",
        "https://alexmacapple.github.io/tester-a11Y/les-formulaires",
        "https://alexmacapple.github.io/tester-a11Y/cas-pratique-5",
        "https://alexmacapple.github.io/tester-a11Y/cas-pratique-6"
      ],
      "startServerCommand": "npm run dev",
      "settings": {
        "onlyAudits": ["accessibility"],
        "onlyCategories": ["accessibility"]
      }
    },
    "upload": {
      "target": "temporary-public-storage"
    },
    "assertions": {
      "assertions": {
        "categories:accessibility": ["error", { "minScore": 1 }]
      }
    }
  }
}
